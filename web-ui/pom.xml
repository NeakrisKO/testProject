    <properties>
        <maven.deploy.skip>false</maven.deploy.skip>
        <org.spring.version>4.3.6.RELEASE</org.spring.version>
        <org.springsecurity.version>4.2.3.RELEASE</org.springsecurity.version>
        <org.hibernate.version>5.2.9.Final</org.hibernate.version>
