        </dependency>
        <dependency>
            <groupId>ru.itb</groupId>
            <artifactId>selenium</artifactId>
            <version>1.0-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>ru.itb.testautomation</groupId>
            <artifactId>svn-harvester</artifactId>
            <version>1.0-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>junit</groupId>
            <artifactId>junit</artifactId>
            <version>4.12</version>
            <scope>test</scope>
        </dependency>

        <!-- com.google.guava -->
